<%- include("partials/header") -%>



 <div class="container">  

  <h1 class="top-header">Dashboard</h1>


    <%  posts.forEach(function(post){ %>

    
        <div class="card">
    
            <div class="card-body">
              <h5 class="card-title"><%= post.title %></h5>

              <p class="card-text"><%= post.content %></p>
              <a href="/stacks/<%=post.author %>" class="card-text" ><%= post.authorName %></a>
            </div>
          </div>
        
        
        
        <%})%>





        
</div> 





</body>






<%- include("partials/footer") -%>